
 <a href="/auth/logout">Logout</a>

<a href="/posts/new" class="btn btn-primary col-sm-1">New Post</a>
<a href="/posts/draft" class="btn btn-info col-sm-1">Draft</a>

<h1>List of My Drafts</h1>
<ul>
  <% for (var i = 0; i < posts.length; i++) { %>
    <% if (posts[i]) { %>
      <li>
        <a href="/posts/post/<%= posts[i].id %>"><%= posts[i].title ? posts[i].title : '(no title)' %></a>
      </li>
    <% } %>
  <% } %>
</ul>
